<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd">
	<enterprise-beans>
		<session>
			<ejb-name>ViewUserInfoServiceBean</ejb-name>
			<ejb-class>de.hpi.sam.rubis.usermgmt.impl.ViewUserInfoServiceBean</ejb-class>
			<env-entry>
				<description>Privacy Level</description>
				<env-entry-name>privacyLevel</env-entry-name>
				<env-entry-type>java.lang.String</env-entry-type>
				<env-entry-value>LOW</env-entry-value>
			</env-entry>
		</session>
	</enterprise-beans>
</ejb-jar>